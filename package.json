{
  "name": "github",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:export && npm run build-storybook",
    "build:export": "next build && next export",
    "build-storybook": "build-storybook -o out/_mlc/storybook",
    "lint": "next lint",
    "dev": "next build && next dev",
    "start": "run-p dev server storybook",
    "start:next": "next start",
    "server": "wait-on http://localhost:3000 && opener http://localhost:3000",
    "storybook": "start-storybook -p 6006 -s public"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "babel-loader": "^8.2.4",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.2",
    "typescript": "4.6.3",
    "wait-on": "^6.0.1",
    "webpack-merge": "^5.8.0"
  }
}
